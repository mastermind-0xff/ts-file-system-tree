{
  "commands": [
    {
      "command": "commands.refresh",
      "text": "$(sync)",
      "tooltip": "Refresh commands",
      "color": "#FFCC00"
    },
    {
      "text": "push x.y.Z",
      "command": "workbench.action.terminal.sendSequence",
      "arguments": [{"text": "npm run push\u000d"}],
      "tooltip": "npm version patch && git push"
    },
    {
      "text": "push x.Y.z",
      "command": "workbench.action.terminal.sendSequence",
      "arguments": [{"text": "npm run push-minor\u000d"}],
      "tooltip": "npm version minor && git push"
    },
  ]
}